    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    
          <nav class="navbar navbar-inverse" style='padding-left:1.5%;'>
            <div class="container-fluid">
              <div class="navbar-header">
                <% if @log_in != true %> <a class="navbar-brand" href="#" style="color: white;font-weight:bold;">Test Results Entry</a> <% end %>
              </div>
              <ul class="nav navbar-nav">
              <% if @log_in != true %>   <li class="active"><a href="#">Enter Results</a></li> <% end %>
               
              </ul>
            </div>
          </nav>
   
    <%= yield %>
  </body>
</html>

